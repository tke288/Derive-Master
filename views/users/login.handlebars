<div class="container">
<div class="row" ng-app="login" ng-controller="loginController">
  <div class="col-md-6 mx-auto">
    <div class="card card-body">
      <img class="logo" src="/img/logo.png">
      <h3 class="text-center">Account Login</h3>
        <div class="form-group">
          <label for="email">Email</label>
          <input class="form-control" ng-model='data.email'>
        </div>
         <div class="form-group">
          <label for="password">Password</label>
          <input type="password" name="password" class="form-control" required ng-model='data.password'>
        </div>
        <button  class="btn btn-primary" ng-click="submit()">Submit</button>
    </div>
  </div>
</div>

</div
